{
  "name": "react-webpack-tpl",
  "version": "0.0.1",
  "description": "Webpack5 based react scaffolding template.",
  "author": "Junting <342766475@qq.com>",
  "repository": "git@github.com:JuntingLiu/react-webpack-tpl.git",
  "main": "index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack server --config build/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "prepare": "husky install"
  },
  "keywords": [
    "webpack5",
    "React18",
    "TypeScript",
    "ReactQuery"
  ],
  "license": "MIT",
  "homepage": "./",
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "chalk": "2.4.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.1.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "install": "^0.13.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.1",
    "terser-webpack-plugin": "^5.3.6",
    "thread-loader": "^3.0.4",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-query-devtools": "^4.20.4",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "antd": "5.x",
    "axios": "^0.27.2",
    "c-log-kit": "^0.0.4",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.4.2"
  }
}
